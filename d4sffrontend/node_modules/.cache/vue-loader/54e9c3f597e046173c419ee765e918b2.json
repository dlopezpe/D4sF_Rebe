m to=\"/map\">{{$t(\"app.dashboard\")}}</cv-header-menu-item>\r\n\r\n        <cv-header-menu :aria-label=\"$t('app.administracion')\" :title=\"$t('app.administracion')\" v-if=\"is_superuser || is_systemadmin|| is_enterpriseadmin\">\r\n          \r\n          <cv-header-menu-item to=\"/admin-enterprises\" v-if=\"is_superuser || is_systemadmin\" @click=\"showAlert\">{{$t(\"app.administracion_empresas\")}}</cv-header-menu-item>\r\n          <cv-header-menu-item to=\"/admin-users\" v-if=\"is_superuser || is_systemadmin\" @click=\"showAlert\">{{$t(\"app.administracion_usuarios\")}}</cv-header-menu-item>\r\n          <cv-header-menu-item :to=\"customLinkEnterprise\" v-else-if=\"is_enterpriseadmin && !is_coop_user\" @click=\"showAlert\">{{$t(\"app.administracion_empresas\")}}</cv-header-menu-item>\r\n          <cv-header-menu-item :to=\"customLinkCoop\" v-else-if=\"is_enterpriseadmin && is_coop_user\" @click=\"showAlert\">{{$t(\"app.administracion_cooperativas\")}}</cv-header-menu-item>\r\n          \r\n          <cv-header-menu-item to=\"/admin-cooperatives\" v-if=\"is_superuser || is_systemadmin\">{{$t(\"app.administracion_cooperativas\")}}</cv-header-menu-item>\r\n        </cv-header-menu>\r\n\r\n        <cv-header-menu-item to=\"/chart\" v-if=\"is_superuser || is_systemadmin || is_enterpriseadmin\">{{$t(\"app.graficos\")}}</cv-header-menu-item>\r\n        <cv-header-menu-item @click=\"showAlert\" v-if=\"is_superuser || is_staff\">{{$t(\"app.alertas\")}}</cv-header-menu-item>\r\n\r\n      </cv-header-nav>\r\n      <template slot=\"header-global\">\r\n        \r\n        <cv-header-global-action aria-label=\"User avatar\" :title=\"$t('app.perfil')\" @click=\"editarPerfil\" aria-controls=\"user-panel\">\r\n          <UserAvatarFilledAlt20 />\r\n        </cv-header-global-action>\r\n        <cv-header-global-action aria-label=\"Logout\" :title=\"$t('app.desconect')\" @click=\"logOut\" aria-controls=\"user-panel\">\r\n          <Logout20 />\r\n        </cv-header-global-action>\r\n      </template>\r\n      <template slot=\"left-panels\" v-if=\"areLeftPanels\">\r\n        <cv-side-nav id=\"side-nav\" fixed>\r\n          <cv-side-nav-items>\r\n            <cv-header-side-nav-items>\r\n              <cv-header-menu-item>{{$t(\"app.dashboard\")}}</cv-header-menu-item>\r\n            </cv-header-side-nav-items>\r\n          </cv-side-nav-items>\r\n        </cv-side-nav>\r\n      </template>\r\n      <template slot=\"right-panels\" v-if=\"areRightPanels\"></template>\r\n    </cv-header>\r\n    <div id=\"nav\" v-if=\"this.$router.currentRoute.name != 'Login'\" v-show=\"this.$router.currentRoute.name != 'ResetPasswordView'\">\r\n      <router-link to=\"/\"></router-link>|\r\n      <router-link to=\"/about\"></router-link>\r\n    </div>\r\n    -->\r\n    <router-view />\r\n    <!--\r\n    <AlertaGeneral ref=\"alertaGeneral\" :tituloAlert=\"mensajeAlerta\" :tipoAlert=\"tipoAlerta\" />\r\n    <EditarPerfil ref=\"editarPerfil\"/>\r\n    -->\r\n  </div>\r\n</template>\r\n<script>\r\nconst style = 'font-weight: bold; font-size: 40px;color: red; text-shadow: 3px 3px 0 rgb(217,31,38) , 6px 6px 0 rgb(226,91,14) , 9px 9px 0 rgb(245,221,8) , 12px 12px 0 rgb(5,148,68) , 15px 15px 0 rgb(2,135,206) , 18px 18px 0 rgb(4,77,145) , 21px 21px 0 rgb(42,21,113)';\r\nconsole.log('%c D4SmartFarming by Smartbits', style);\r\n//import AlertaGeneral from \"./components/AlertaGeneral\";\r\n//import EditarPerfil from \"./components/EditarPerfil\";\r\n//import axios from \"axios\";\r\n/*\r\nexport default {\r\n  data() {\r\n    return {\r\n      areRightPanels: false,\r\n      areLeftPanels: true,\r\n      ocultaPaneles: true,\r\n      mensajeAlerta: \"\",\r\n      tipoAlerta: \"\",\r\n      systemadmin: '' ,\r\n      customLinkEnterprise: '',\r\n      customLinkCoop: '',\r\n      is_superuser: false,\r\n      is_staff: false,\r\n      is_systemadmin: false,\r\n      is_enterpriseadmin: false,\r\n      is_coop_user: false\r\n    };\r\n  },\r\n  components: {\r\n        AlertaGeneral,\r\n        EditarPerfil\r\n    },\r\n  methods: {\r\n    cambiaLang(){\r\n      if(this.$session.get('user')){\r\n        const path = `${this.$apiURL}/profiledata/${this.$session.get('user')}/`;\r\n        axios.defaults.headers.common['Authorization'] = \"Bearer \"+sessionStorage.getItem(\"apiAccess\")\r\n        axios\r\n        .get(path)\r\n        .then(response =>{\r\n            this.$root.$i18n.locale = response.data.language\r\n        })\r\n      }else{\r\n        var userLang = navigator.language || navigator.userLanguage; \r\n        if (userLang == 'es-ES'){\r\n            this.$root.$i18n.locale = 'es'\r\n        }else{\r\n            this.$root.$i18n.locale = 'en'\r\n        }\r\n      }\r\n    },\r\n    toUrl(url){\r\n      this.$router.push(url);\r\n    },\r\n    editarPerfil(){\r\n      this.$refs.editarPerfil.openModal()\r\n    },\r\n    showAlert(){\r\n      this.mensajeAlerta = \"En desarrollo\"\r\n      this.tipoAlerta = 'info'\r\n      this.$refs.alertaGeneral.verAlerta()\r\n    },\r\n    showMenu(){\r\n      this.areLeftPanels =  true\r\n    },\r\n    logOut(){\r\n      sessionStorage.removeItem(\"apiAccess\");\r\n      this.$session.destroy()\r\n      this.$router.push(\"/login\");\r\n    },\r\n    compruebaLogin(){\r\n      console.info('comprueba_login')\r\n    },\r\n    isMobile() {\r\n        if(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {\r\n            return true\r\n        } else {\r\n            return false\r\n        }\r\n    }\r\n    \r\n  },\r\n  mounted: function () {\r\n    if(!sessionStorage.getItem(\"apiAccess\") && this.$router.currentRoute.name != 'ResetPasswordView'){\r\n      this.$router.push(\"/login\");\r\n    }else{\r\n      const path = `${this.$apiURL}/profiledata/${this.$session.get('user')}/`;\r\n      axios.defaults.headers.common['Authorization'] = \"Bearer \"+sessionStorage.getItem(\"apiAccess\")\r\n      .get(path)\r\n      .then(response =>{\r\n          console.info('salida')\r\n          console.info(response)\r\n      })\r\n      .catch((error) => console.info(error))\r\n    }\r\n    this.$session.get('is_systemadmin')\r\n    \r\n    if(this.$session.get('is_enterpriseadmin')){\r\n      this.customLinkEnterprise = 'edit-enterprise?enterprise='+this.$session.get('enterprise')\r\n    }\r\n    if(this.$session.get('is_coop_user')){\r\n      this.customLinkCoop = 'edit-cooperative?cooperative='+this.$session.get('enterprise')\r\n    }\r\n    var cookie = sessionStorage.getItem(\"apiAccess\")\r\n    \r\n    if(this.$session.get('user')){\r\n      const path = `${this.$apiURL}/profiledata/${this.$session.get('user')}/`;\r\n      axios.defaults.headers.common['Authorization'] = \"Bearer \"+sessionStorage.getItem(\"apiAccess\")\r\n      axios\r\n      .get(path)\r\n      .then(response =>{\r\n          this.$root.$i18n.locale = response.data.language\r\n      })\r\n    }\r\n    if(cookie != null || this.$router.currentRoute.name == 'ResetPasswordView'){\r\n      console.info('User Logged')\r\n      const path = `${this.$apiURL}/profiledata/${this.$session.get('user')}/`;\r\n      axios.defaults.headers.common['Authorization'] = \"Bearer \"+sessionStorage.getItem(\"apiAccess\")\r\n      .get(path)\r\n      .then(response =>{\r\n          console.info('salida')\r\n          console.info(response)\r\n      })\r\n      .catch((error) => console.info(error))\r\n    }else{\r\n      this.$router.push(\"/login\");\r\n    }\r\n  },\r\n  created(){\r\n    if(!sessionStorage.getItem(\"apiAccess\") && this.$router.currentRoute.name != 'ResetPasswordView'){\r\n      console.warn('error')\r\n      sessionStorage.clear();\r\n      this.$session.destroy()\r\n      this.$router.push(\"/login\");\r\n    }\r\n\r\n    this.$session.get('is_systemadmin')\r\n    if(this.$session.get('is_enterpriseadmin')){\r\n      this.customLinkEnterprise = 'edit-enterprise?enterprise='+this.$session.get('enterprise')\r\n    }\r\n    if(this.$session.get('is_coop_user')){\r\n      this.customLinkCoop = 'edit-cooperative?cooperative='+this.$session.get('enterprise')\r\n    }\r\n    var cookie = sessionStorage.getItem(\"apiAccess\")\r\n    if(this.$session.get('user')){\r\n      console.info('entra')\r\n      const path = `${this.$apiURL}/profiledata/${this.$session.get('user')}/`;\r\n      axios.defaults.headers.common['Authorization'] = \"Bearer \"+sessionStorage.getItem(\"apiAccess\")\r\n      axios\r\n      .get(path)\r\n      .then(response =>{\r\n          this.$root.$i18n.locale = response.data.language\r\n      })\r\n      .catch((error) => {console.info(error)})\r\n    }\r\n    if(cookie != null || this.$router.currentRoute.name == 'ResetPasswordView'){\r\n      console.info('User Logged')\r\n    }else{\r\n      this.$router.push(\"/login\");\r\n    }\r\n  }\r\n}\r\n*/\r\n</script>\r\n<style>\r\n/*\r\n#app {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n}\r\n*/\r\n#nav {\r\n  padding: 17px;\r\n}\r\n\r\n#nav a {\r\n  font-weight: bold;\r\n  color: #2c3e50;\r\n}\r\n\r\n#nav a.router-link-exact-active {\r\n  color: #42b983;\r\n}\r\n.bx--slider-text-input{\r\n    display: none;\r\n}\r\n#colSlider > .cv-slider > .bx--slider-container{\r\n  width: 100%;\r\n}\r\n#inputCursor{\r\n  cursor: not-allowed;\r\n}\r\n.bx--slider__filled-track{\r\n  background: #0f62fe!important;\r\n}\r\n</style>\r\n"]}]}